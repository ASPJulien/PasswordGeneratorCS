<Window x:Class="Password_generator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Password_generator"
        mc:Ignorable="d"
        Title="Password generator" Height="290" Width="265" ResizeMode="NoResize">
    <Grid>
        <Label Content="Password generator&#xD;&#xA;" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="184" Height="37" FontSize="20"/>
        <RadioButton Content="Low" HorizontalAlignment="Left" Margin="65,93,0,0" VerticalAlignment="Top" Checked="RadioButton_Checked"/>
        <Label Content="Security:" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top"/>
        <RadioButton Content="High" HorizontalAlignment="Left" Margin="110,93,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.045,0.8" Checked="RadioButton_Checked_1"/>
        <RadioButton Content="Very High" HorizontalAlignment="Left" Margin="159,93,0,0" VerticalAlignment="Top" Checked="RadioButton_Checked_2"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="10,126,0,0" VerticalAlignment="Top"/>
        <Button Content="Generate" HorizontalAlignment="Left" Margin="34,201,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
        <TextBox Name="Result" HorizontalAlignment="Left" Height="23" Margin="34,173,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" AutomationProperties.Name="Result" IsReadOnly="True"/>

    </Grid>
</Window>
